{
  "name": "seed-icons",
  "version": "1.1.1",
  "description": "An open-source icon set that makes tracking down and using optimized, quality, consistent brand SVG icons a little less painful.",
  "main": "dist/seed.js",
  "unpkg": "dist/seed.min.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "./scripts/build.sh",
    "official": "npm i && npm run build && npm publish",
    "format": "npx prettier --check --write package scripts seedicons.com/src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/seedicons/seed.git"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.10.2",
    "@babel/node": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "@babel/register": "^7.10.1",
    "babel-loader": "^8.1.0",
    "cheerio": "^1.0.0-rc.3",
    "classnames": "^2.2.6",
    "html-minifier": "^4.0.0",
    "prettier": "^2.0.5",
    "svgo": "^1.3.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  },
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true
  },
  "author": "Ross Moody <ross@rossmoody.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/seedicons/seed/issues"
  },
  "homepage": "https://seedicons.com"
}
