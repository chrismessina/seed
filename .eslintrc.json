{
  "extends": ["airbnb-base", "prettier"],
  "plugins": ["import", "prettier"],
  "rules": {
    "no-param-reassign": "off",
    "no-shadow": "off",
    "no-use-before-define": "off",
    "no-undef": "off",
    "no-console": "off",
    "import/no-extraneous-dependencies": "off",
    "prettier/prettier": [
      "error",
      {
        "singleQuote": true,
        "semi": false
      }
    ]
  }
}
